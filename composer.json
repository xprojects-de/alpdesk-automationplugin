{
  "name": "alpdesk/alpdesk-automationplugin",
  "description": "Alpdesk-Automationplugin for Alpdesk",
  "keywords": [
    "contao",
    "alpdesk"
  ],
  "type": "contao-bundle",
  "version": "0.1.4",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "alpdesk/alpdesk-core": "^2.0.0"
  },
  "authors": [
    {
      "name": "Alpdesk",
      "homepage": "https://alpdesk.de",
      "email": "infos@alpdesk.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Alpdesk\\AlpdeskAutomationPlugin\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Alpdesk\\AlpdeskAutomationPlugin\\ContaoManager\\Plugin",
    "contao-component-dir": "public"
  },
  "config": {
    "contao-component-dir": "public"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.81",
    "contao/manager-bundle": "^4.11",
    "vimeo/psalm": "^4.6",
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^5.2"
  }
}