{
  "name": "alpdesk/alpdesk-automationplugin",
  "description": "Alpdesk-Automationplugin for Alpdesk",
  "keywords": [
    "contao",
    "alpdesk"
  ],
  "type": "contao-bundle",
  "version": "1.0.0",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "alpdesk/alpdesk-core": "^2.0 || ^3.0"
  },
  "authors": [
    {
      "name": "Alpdesk",
      "homepage": "https://alpdesk.de",
      "email": "infos@alpdesk.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Alpdesk\\AlpdeskAutomationPlugin\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Alpdesk\\AlpdeskAutomationPlugin\\ContaoManager\\Plugin",
    "contao-component-dir": "public"
  },
  "config": {
    "contao-component-dir": "public",
    "allow-plugins": true
  },
  "require-dev": {
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpstan/phpstan-symfony": "^1.0",
    "slam/phpstan-extensions": "^6.0",
    "contao/manager-bundle": "^4.13 || ^5.0",
    "contao/manager-plugin": "^2.0",
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^5.4 || ^6.0",
    "rector/rector": "^0.13.10"
  }
}